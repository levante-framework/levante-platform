{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": ["functions/levante-admin/lib/**", "functions/levante-assessment/lib/**"]
    },
    "build:dev": {
      "outputs": ["dist/**"]
    },
    "test": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:db": {
      "cache": false,
      "persistent": true
    },
    "emulator:seed": {
      "outputs": ["emulator_data/**"],
      "cache": false,
      "dependsOn": ["build"]
    },
    "emulator:clear": {
      "cache": false
    },
    "emulator:export": {
      "cache": false
    },
    "emulator:reset": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
