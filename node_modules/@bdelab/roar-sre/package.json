    "postversion": "GIT_REMOTE=$(git remote -v | grep \"yeatmanlab/roar-sre.git \\(push\\)\" | cut -f 1) && git push $GIT_REMOTE && git push $GIT_REMOTE --tags",
    "predev": "kill-port 8000 || true",
